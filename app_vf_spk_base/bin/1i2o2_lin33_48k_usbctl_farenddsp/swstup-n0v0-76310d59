#define _SwitchSetup_h_ 





typedef struct
{
  unsigned int prev_routing_id;
  unsigned int prev_link_id;
  unsigned int prev_link_config;
  unsigned int prev_link_direction;
  unsigned int prev_direction_bits_0;
  unsigned int prev_direction_bits_1;
  unsigned int next_boot_mode;
  unsigned int next_routing_id;
  unsigned int next_link_id;
  unsigned int next_link_config;
  unsigned int next_link_direction;
  unsigned int next_direction_bits_0;
  unsigned int next_direction_bits_1;
  unsigned int pll_type;
  unsigned int pll_value;
  unsigned int refdiv_val;
  unsigned int switchdiv_val;
  unsigned int tile0div_val;
  unsigned int tile1div_val;
  unsigned int reset_delay;
  unsigned int link_speed_slow;
  unsigned int link_latency_delay;
  unsigned int link_latency_delay_slow;
} Spanning_Table_T;

typedef struct
{
  unsigned int routing_id;
  unsigned int dimension0;
  unsigned int dimension1;
  unsigned int link_latency_delay;
  unsigned int link_latency_delay_slow;
} Spanning_Routing_Table_T;

typedef struct
{
  unsigned int target_link_num;
  unsigned int target_link_config;
  unsigned int target_link_direction;
} Link_Info_T;

typedef struct
{
  unsigned int target_routing_id;
  unsigned int new_routing_id;
  unsigned int target_num_links;
  unsigned int target_link_info_index;
  unsigned int target_direction_bits_0;
  unsigned int target_direction_bits_1;
  unsigned int target_unused_link_mask;
  unsigned int link_latency_delay;
} Links_Table_T;

typedef struct
{
  unsigned int target_routing_id;
  unsigned int target_galaxian_config;
  unsigned int target_galaxian_freq;
} Galaxian_Table_T;






void SwitchSetup_SetMasterPLL(unsigned int pll, unsigned int ref_div);

void SwitchSetup_DoSingleTileConfig(void);






void SwitchSetup_SetMasterPLL_RGMII(unsigned int pll,
                                    unsigned int ref_div,
                                    unsigned int switch_div,
                                    unsigned int tile0_div,
                                    unsigned int tile1_div);
  void SwitchSetup_DoMultiTileConfig(Spanning_Table_T spanning_table [num_spanning_tables],
                                     unsigned int num_spanning_tables,
                                     Spanning_Routing_Table_T routing_table [num_routing_tables],
                                     unsigned int num_routing_tables);

  void SwitchSetup_DoLinkConfig(Links_Table_T links_table [num_link_tables],
                                Link_Info_T link_info [],
                                unsigned int num_link_tables);

  void SwitchSetup_DoGalaxianConfig(Galaxian_Table_T galaxian_table [num_galaxian_tables],
                                    unsigned int num_galaxian_tables);

