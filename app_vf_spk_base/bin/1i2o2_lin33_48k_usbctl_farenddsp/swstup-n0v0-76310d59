////////////////////////////////////////////////////////////////////////////////
//spanning tables and link setup tables

unsigned int master_routing_id = 0x8002;
unsigned int spanning_routing_id = 0x0000;
Link_Info_T link_info [] =
{
  {0x00000008, 0xc0019833, 0x00000001},
}; 

Links_Table_T links_table [] =
{
  {0x00000000, 0x00008002, 0x00000001, 0, 0x22222102, 0x22222222, 0x000000ff, 0x00000000},
};

unsigned int num_link_tables = 0x00000001;

Galaxian_Table_T galaxian_table [] =
{
  {0x00008006, 0xc1000000, 0x00000018},
};

unsigned int num_galaxian_tables = 1;

////////////////////////////////////////////////////////////////////////////////
//SwitchSetup_DoSwitchSetup

void SwitchSetup_DoSwitchSetup(void)
{
  //Program master tile pll and ref_div
  unsigned int master_pll = 0x80003100;
  unsigned int master_ref_div = 0x00000005;
  SwitchSetup_SetMasterPLL(master_pll, master_ref_div);

  //Switch Setup
  SwitchSetup_DoSingleTileConfig();

  SwitchSetup_DoLinkConfig(links_table, link_info, num_link_tables);

  SwitchSetup_DoGalaxianConfig(galaxian_table, num_galaxian_tables);

}

